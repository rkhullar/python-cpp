#!/usr/bin/env python

import tpg

ops =	{
			'+': lambda x,y: x+y,
			'-': lambda x,y: x-y
		}
def op(sym):
	return ops[sym]


class Adder(tpg.Parser):
	r"""

	separator	spaces	:	'\s+'	;
	token		number	:	'\d+'	$ int
	token		add		:	'[+-]'	$ op
									;

	START/x	->	SB/b						$ x = b
			|	SA/a						$ x = a
			;
	SA/x	->	number/a					$ x = a			$;
	SB/x	->	number/a add/op number/b	$ x = op(a,b)	$;
	"""

t = Adder()

while 1:
	l = raw_input(":")
	if l:
		try:
			print t(l)
		except Exception:
			print tpg.exc()
	else:
		break
