#!/usr/bin/env python

import tpg

class Test(tpg.Parser):
	r"""

	separator spaces: '\s+' ;
	token number: '\d+' int ;

	START/e -> number/n $e = n$;

	"""

t = Test()

while 1:
	l = raw_input(":")
	if l == 'exit':
		break
	if l:
		try:
			print t(l)
		except Exception:
			print tpg.exc()
	else:
		break
